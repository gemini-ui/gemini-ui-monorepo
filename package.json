    "change-case": "^5.4.2",
    "husky": "^8.0.3",
    "lint-staged": "^15.2.0",
    "prettier": "^3.1.1",
    "prettier-plugin-astro": "^0.12.3",
    "sass": "^1.69.7",
    "svgo": "^3.2.0"
  }
}
